<h1>Listing projects</h1>

<%= form_tag "/projects", method: 'GET', id: 'projects_search' do %>
  <%= text_field_tag 'q', params[:q] %>
  <%= submit_tag 'SEARCH' %>
<% end %>


<br/><br/>

<div id="projects">
  <%= render "projects" %>
</div>

<br/>


<br/>

<%= link_to 'New Project', new_project_path %>
